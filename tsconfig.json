{
    "compilerOptions": {
      "target": "ESNext",
      "useDefineForClassFields": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["vite/client"]
    },
    "include": ["src", "vite-env.d.ts"],
    "references": [{ "path": "./tsconfig.node.json" }]
  }
  